      <ID>619</ID>
      <DefaultText>Stunning Scream</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>620</ID>
      <DefaultText>Drake area of effect Burn Attack</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
